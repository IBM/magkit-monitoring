<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
  <name>monitoring</name>
  <displayName>MagKit Monitoring Module</displayName>
  <description>Monitoring Module for Magnolia.</description>
  <class>com.aperto.magkit.monitoring.MonitoringModule</class>
  <versionHandler>com.aperto.magkit.monitoring.MonitoringModuleVersionHandler</versionHandler>
  <version>${parsedVersion.majorVersion}.${parsedVersion.minorVersion}.${parsedVersion.incrementalVersion}-${build.buildNumber}</version>

  <components>
